	<script type="text/javascript">
		function fn_sample1(money) {
			if(money > 5000) {
				alert('엄청 부자시네요.');
			}
			else if(money > 3000) {
				alert('조금 부자시네요.');
			}
			else {
				alert('음...');
			}
		}
		
		fn_sample1(5000);
	</script>
